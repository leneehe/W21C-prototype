<section data-spy="scroll" data-target="#mainmenu" data-offset="50">
<!-- preloader -->
  <div class="preloade">
      <span class="text-center"><%= image_tag("MCC_logo_50x50.png") %></span>
  </div>
 <!--Mainmenu-->
  <nav class="navbar navbar-default mainmenu-area navbar-fixed-top" data-spy="affix" data-offset-top="60">
      <div class="container">
          <div class="navbar-header">
              <button type="button" data-toggle="collapse" class="navbar-toggle" data-target="#mainmenu">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
              </button>

              <%= link_to root_path, class: "navbar-brand" do %>
                <%= image_tag "mycarecompass_logo.png" %>
              <% end %>
              <%= link_to root_path, class: "sm-brand" do %>
                <%= image_tag "MCC_logo_50x50.png" %>
              <% end %>
          </div>
          <div class="collapse navbar-collapse navbar-right" id="mainmenu">
              <ul class="nav navbar-nav">
                  <li><%= link_to "Home", root_path %></li>

                  <li><%= link_to "Plan", dashboard_plan_path %></li>
                  <!-- <li><%# link_to "Plan", plan_path %></li> -->

                  <li><%= link_to "Goal Centre", dashboard_goals_path %></li>
                  <!-- <li><%# link_to "Goal Centre", goal_centre_path %></li> -->

                  <li><%= link_to "Track", symptoms_path %></li>
                  <!-- <li><%# link_to "Track", track_path %></li> -->

                  <li><%= link_to "Learn", learn_path %></li>
                  <% if user_signed_in? %>
                    <li><%= link_to "Resources", dashboard_resources_path %></li>
                  <% else %>
                    <li><%= link_to "Resources", resources_path %></li>
                  <% end %>
                  <li><%= link_to "About", about_path %></li>
                <% if !user_signed_in? %>
                    <li><%= link_to "Sign Up", new_user_registration_path %></li>
                    <li><%= link_to "Login", new_user_session_path %></li>
                <% else %>
                    <li><%= link_to "Logout", destroy_user_session_path%></li>
                    <li><%= link_to "Profile", user_path(current_user)%></li>
                <% end %>
              </ul>
          </div>
      </div>
  </nav>
</section>
